{
  "name": "pdf-img-api",
  "version": "1.0.0",
  "description": "http host api for convert pdf to png ",
  "private": true,
  "egg": {
    "declarations": true,
    "tsHelper": {
      "watchDirs": {
        "util": {
          "enabled": true,
          "directory": "app/util",
          "declareTo": "util"
        }
      }
    }
  },
  "dependencies": {
    "canvas": "^2.8.0",
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0",
    "fs-extra": "^10.0.0",
    "pdf2pic": "^2.1.4",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "egg-ts-helper": "^1.29.1",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-pdf-img-api",
    "stop": "egg-scripts stop --title=egg-server-pdf-img-api",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "pxy",
  "license": "MIT"
}
